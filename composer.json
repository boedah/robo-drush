{
    "name": "boedah/robo-drush",
    "description": "Drush CommandStack for Robo Task Runner",
    "type": "robo-tasks",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Gasser",
            "email": "dev@boedah.de"
        }
    ],
    "scripts": {
        "test": "robo --ansi test"
    },
    "autoload": {
        "psr-4": {
            "Boedah\\Robo\\Task\\Drush\\": "src"
        }
    },
    "require": {
        "php": ">=8.2",
        "consolidation/robo": "^4.0.6 || ^5.1.0"
    },
    "require-dev": {
        "drush/drush": "^12 || ^13",
        "drupal/core": "^10 || ^11"
    },
    "suggest": {
        "drush/drush": "robo-drush needs a global or local Drush to use."
    },
    "conflict": {
        "drush/drush": "<12"
    },
    "config": {
        "allow-plugins": {
            "drupal/core-composer-scaffold": false
        }
    }
}
